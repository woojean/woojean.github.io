    <span class="n">tmpArray</span><span class="p">[</span> <span class="n">tmpPos</span><span class="o">++</span> <span class="p">]</span> <span class="o">=</span> <span class="n">arr</span><span class="p">[</span> <span class="n">lBegin</span><span class="o">++</span> <span class="p">];</span>

  <span class="c1">// 14,7,3,1</span>
  <span class="k">for</span><span class="p">(</span><span class="n">incr</span> <span class="o">=</span> <span class="n">len</span><span class="o">/</span><span class="mi">2</span><span class="p">;</span> <span class="n">incr</span> <span class="o">&gt;</span> <span class="mi">0</span><span class="p">;</span> <span class="n">incr</span> <span class="o">/=</span> <span class="mi">2</span><span class="p">){</span>
